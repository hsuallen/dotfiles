[user]
	name = Mitchell Kember
	email = mk12360@gmail.com
[alias]
	s = status -s
	b = branch
	c = checkout
	ap = add --patch
	cm = commit
	rb = rebase
	unstage = reset HEAD --
	last = log -1 HEAD
[color]
	ui = auto
[core]
	editor = vim
	excludesfile = ~/.gitignore
[merge]
	tool = opendiff
	conflictstyle = diff3
[push]
	default = simple
[pull]
	rebase = true
[apply]
	whitespace = fix
